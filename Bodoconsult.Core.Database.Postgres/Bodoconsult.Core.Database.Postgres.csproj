  </ItemGroup>

  <ItemGroup>
    <Content Remove="C:\Users\rleisner\.nuget\packages\bodoconsult.core.database\1.0.5\contentFiles\any\netcoreapp3.1\LICENSE.md" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE.md" Link="LICENSE.md" />
