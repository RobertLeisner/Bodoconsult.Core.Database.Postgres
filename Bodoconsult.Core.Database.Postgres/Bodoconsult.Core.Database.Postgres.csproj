    <PackageReleaseNotes>Enahnceing metadat infrastructure and bugfixingha.</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/RobertLeisner/Bodoconsult.Core.Database.Postgres</PackageProjectUrl>

  </PropertyGroup>

